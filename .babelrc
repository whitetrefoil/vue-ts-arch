{
  "presets"      : [
    [
      "@babel/env",
      {
        "targets"           : {
          "browsers": "Android >= 5.0, iOS >= 8.0, last 5 Chrome versions, last 5 Firefox versions, last 5 Opera versions, IE >= 10, last 2 Edge versions"
        },
        "modules"           : false,
        "useBuiltIns"       : "entry"
      }
    ]
  ],
  "compact"      : true,
  "plugins"      : [
    "lodash",
    "transform-node-env-inline",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-syntax-dynamic-import"
  ],
  "highlightCode": false,
  "minified"     : false,
  "comments"     : true,
  "env"          : {
    "test": {
      "presets": [
        [
          "@babel/env",
          {
            "targets"    : {
              "browsers": "Android >= 5.0, iOS >= 8.0, last 5 Chrome versions, last 5 Firefox versions, last 5 Opera versions, IE >= 10, last 2 Edge versions"
            },
            "modules"    : "commonjs",
            "useBuiltIns": false
          }
        ]
      ],
      "plugins": [
        "lodash",
        "@babel/plugin-proposal-object-rest-spread",
        "dynamic-import-node"
      ]
    }
  }
}
