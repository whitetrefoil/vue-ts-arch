{
  "presets"      : [
    [
      "env",
      {
        "targets"    : {
          "browsers": "Android >= 4.3, iOS >= 8.0, last 5 Chrome versions, last 5 Firefox versions, last 5 Opera versions, IE >= 10, last 5 Edge versions"
        },
        "modules"    : false,
        "useBuiltIns": true
      }
    ]
  ],
  "compact"      : false,
  "plugins"      : [
    "lodash",
    "syntax-dynamic-import"
  ],
  "highlightCode": false,
  "sourceMaps"   : true,
  "minified"     : false,
  "comments"     : true
}
