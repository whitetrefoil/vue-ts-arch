{
  "name": "vue-ts-arch",
  "version": "0.8.0",
  "main": "src/entry.js",
  "description": "A basic project structure built with Vue & TS.",
  "author": "WhiteTrefoil <whitetrefoil@gmail.com>",
  "private": true,
  "license": "Unlicense",
  "scripts": {
    "build": "gulp build",
    "start": "gulp serve -d",
    "serve": "gulp serve -d",
    "test": "rimraf test_results && jest --no-cache --no-coverage; tslint -p tsconfig.json",
    "coverage": "rimraf test_results && jest --no-cache; tslint -p tsconfig.json"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.42",
    "av-ts": "^0.9.3",
    "axios": "^0.18.0",
    "axios-retry": "^3.0.2",
    "bootstrap": "^4.0.0",
    "debug": "^3.1.0",
    "lodash": "^4.17.5",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@types/body-parser": "^1.16.8",
    "@types/clear-require": "^2.0.0",
    "@types/connect": "^3.4.31",
    "@types/connect-history-api-fallback": "^1.3.1",
    "@types/core-js": "^0.9.46",
    "@types/debug": "^0.0.30",
    "@types/del": "^3.0.1",
    "@types/extract-text-webpack-plugin": "^3.0.2",
    "@types/fancy-log": "^1.3.0",
    "@types/gulp": "^4.0.5",
    "@types/gulp-connect": "^5.0.2",
    "@types/html-webpack-plugin": "^2.30.3",
    "@types/http-proxy": "^1.16.0",
    "@types/http-proxy-middleware": "^0.17.4",
    "@types/jest": "^22.2.2",
    "@types/koa": "^2.0.44",
    "@types/lodash": "^4.14.106",
    "@types/lodash-webpack-plugin": "^0.11.3",
    "@types/node": "^9.6.0",
    "@types/webpack": "^4.1.3",
    "@types/webpack-dev-middleware": "^2.0.1",
    "@types/webpack-dev-server": "^2.9.4",
    "ajv": "^6.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-lodash": "^3.3.2",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.2",
    "clear-require": "^2.0.0",
    "connect-history-api-fallback": "^1.5.0",
    "core-js": "^2.5.4",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fancy-log": "^1.3.2",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "gulp": "^4.0.0",
    "gulp-connect": "^5.5.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.1.0",
    "http-proxy": "^1.16.2",
    "http-proxy-middleware": "^0.18.0",
    "jest": "^22.4.3",
    "jest-environment-jsdom-global": "^1.0.3",
    "jest-junit": "^3.6.0",
    "jest-localstorage-mock": "^2.2.0",
    "lodash-webpack-plugin": "^0.11.4",
    "koa-connect": "^2.0.1",
    "meow": "^4.0.0",
    "merge-stream": "^1.0.1",
    "mock-server-middleware": "^0.4.0-alpha.4",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.8.3",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.3",
    "time-stamp": "^2.0.0",
    "ts-jest": "^22.4.2",
    "ts-loader": "^4.1.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.6.0",
    "typescript": "^2.8.1",
    "url-loader": "^1.0.1",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.3.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-server": "^3.1.1",
    "webpack-serve": "^0.3.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/~.*"
    ],
    "coverageDirectory": "test_results",
    "coverageReporters": [
      "clover",
      "lcov",
      "text-summary"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tests/tsconfig.json",
        "skipBabel": false,
        "useBabelrc": false
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^vue$": "<rootDir>/node_modules/vue/dist/vue.runtime.esm.js",
      "^vue-router$": "<rootDir>/node_modules/vue-router/dist/vue-router.esm.js",
      "^vuex$": "<rootDir>/node_modules/vuex/dist/vuex.esm.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/file-mock.ts",
      "\\.(css|less|sass|scss)$": "<rootDir>/tests/style-mock.ts"
    },
    "setupFiles": [
      "@babel/polyfill",
      "jest-localstorage-mock",
      "<rootDir>/tests/unhandled-promise-rejection.ts"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/tests/setup.ts",
    "testEnvironment": "jest-environment-jsdom-global",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "testResultsProcessor": "jest-junit",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!vue)"
    ]
  },
  "jest-junit": {
    "output": "./test_results/junit/junit.xml",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "false"
  }
}
