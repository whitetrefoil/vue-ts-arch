  "description": "A basic project structure built with Vue & TS.",
  "author": "WhiteTrefoil <whitetrefoil@gmail.com>",
  "private": true,
  "license": "Unlicense",
  "scripts": {
    "build": "build",
    "start": "gulp serve",
    "serve": "gulp serve -d",
    "test": "rimraf test_results && jest --no-cache --no-coverage",
    "coverage": "rimraf test_results && jest --no-cache"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "dependencies": {
    "av-ts": "^0.9.2",
    "axios": "^0.17.1",
    "axios-retry": "^3.0.1",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.0.0-alpha.6",
    "debug": "^3.1.0",
    "lodash": "^4.17.4",
    "r2": "^2.0.0",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/clear-require": "^2.0.0",
    "@types/core-js": "^0.9.44",
    "@types/debug": "^0.0.30",
    "@types/del": "^3.0.0",
    "@types/extract-text-webpack-plugin": "^3.0.0",
    "@types/gulp": "^4.0.5",
    "@types/gulp-connect": "^5.0.1",
    "@types/gulp-util": "^3.0.34",
    "@types/html-webpack-plugin": "^2.30.1",
    "@types/http-proxy": "^1.12.3",
    "@types/jest": "^22.0.1",
    "@types/lodash": "^4.14.92",
    "@types/lodash-webpack-plugin": "^0.11.1",
    "@types/minimist-options": "^3.0.0",
    "@types/node": "^8.5.7",
    "@types/run-sequence": "0.0.30",
    "@types/webpack": "^3.8.2",
    "@types/webpack-dev-server": "^2.9.2",
    "ajv": "^6.0.0",
    "awesome-typescript-loader": "^3.4.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.0",
    "clear-require": "^2.0.0",
    "core-js": "^2.5.3",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.8",
    "del": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "gulp": "^3.9.1",
    "gulp-connect": "^5.2.0",
    "gulp-util": "^3.0.8",
    "html-loader": "^0.5.4",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy": "^1.16.2",
    "jest": "^22.0.4",
    "jest-localstorage-mock": "^2.1.0",
    "lodash-webpack-plugin": "^0.11.4",
    "meow": "^4.0.0",
    "merge-stream": "^1.0.1",
    "mock-server-middleware": "^0.4.0-alpha.4",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.7.2",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.2.1",
    "rimraf": "^2.6.2",
    "run-sequence": "^2.2.1",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.3",
    "time-stamp": "^2.0.0",
    "ts-jest": "^22.0.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.6.2",
    "uglify-es": "^3.3.5",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.6.2",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.10.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/~.*"
    ],
    "coverageDirectory": "test_results",
    "coverageReporters": [
      "clover",
      "lcov",
      "text-summary"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tests/tsconfig.json",
        "skipBabel": false,
        "useBabelrc": true
      }
    },
    "mapCoverage": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/file-mock.ts",
      "\\.(css|less|sass|scss)$": "<rootDir>/tests/style-mock.ts"
    },
    "setupFiles": [
      "babel-polyfill/lib/index.js",
      "jest-localstorage-mock",
      "<rootDir>/tests/unhandled-promise-rejection.ts"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
