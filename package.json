    "ts-loader": "^6.0.3",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.5.2",
    "url-loader": "^2.0.0",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.35.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-dev-server": "^3.7.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/~.*"
    ],
    "coverageDirectory": "test_results",
    "coverageReporters": [
      "clover",
      "lcov",
      "text-summary"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true,
        "tsConfig": "tests/tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^vue$": "<rootDir>/node_modules/vue/dist/vue.runtime.esm.js",
      "^vue-router$": "<rootDir>/node_modules/vue-router/dist/vue-router.esm.js",
      "^vuex$": "<rootDir>/node_modules/vuex/dist/vuex.esm.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/file-mock.ts",
      "\\.(css|less|sass|scss)$": "<rootDir>/tests/style-mock.ts"
    },
    "preset": "ts-jest",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupFiles": [
      "@babel/polyfill",
      "jest-localstorage-mock",
      "<rootDir>/tests/unhandled-promise-rejection.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ],
    "testEnvironment": "jest-environment-jsdom-global",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!vue)"
    ]
  },
  "jest-junit": {
    "output": "./test_results/junit/junit.xml",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "false"
  }
}
