    "@types/debug": "^4.1.4",
    "@types/fancy-log": "^1.3.1",
    "@types/fs-extra": "^7.0.0",
    "@types/gulp": "^4.0.6",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^24.0.15",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^5.0.1",
    "@types/koa-mount": "^3.0.1",
    "@types/koa-router": "^7.0.41",
