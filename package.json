    "@types/lodash-webpack-plugin": "^0.11.3",
    "@types/mini-css-extract-plugin": "^0.2.1",
    "@types/mockjs": "^1.0.2",
    "@types/node": "^12.0.8",
    "@types/shortid": "0.0.29",
